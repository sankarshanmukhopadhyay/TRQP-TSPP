{
  "$id": "https://example.org/schemas/tspp-conformance-report.schema.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "additionalProperties": false,
  "properties": {
    "generated_at": {
      "type": "string"
    },
    "profile": {
      "type": "string"
    },
    "results": {
      "items": {
        "additionalProperties": false,
        "properties": {
          "duration_seconds": {
            "minimum": 0,
            "type": "number"
          },
          "name": {
            "type": "string"
          },
          "nodeid": {
            "type": "string"
          },
          "notes": {
            "type": [
              "string",
              "null"
            ]
          },
          "outcome": {
            "$ref": "../../schemas/core/test_outcome.schema.json"
          },
          "requirement_ids": {
            "items": {
              "type": "string"
            },
            "type": "array"
          }
        },
        "required": [
          "nodeid",
          "name",
          "outcome",
          "duration_seconds",
          "requirement_ids"
        ],
        "type": "object"
      },
      "type": "array"
    },
    "summary": {
      "additionalProperties": false,
      "properties": {
        "ERROR": {
          "type": "integer"
        },
        "FAIL": {
          "type": "integer"
        },
        "NOT_APPLICABLE": {
          "type": "integer"
        },
        "PASS": {
          "type": "integer"
        },
        "SKIP": {
          "type": "integer"
        },
        "XFAIL": {
          "type": "integer"
        },
        "exit_status": {
          "type": "integer"
        }
      },
      "required": [
        "exit_status",
        "PASS",
        "FAIL",
        "SKIP",
        "NOT_APPLICABLE",
        "ERROR",
        "XFAIL"
      ],
      "type": "object"
    },
    "target": {
      "additionalProperties": true,
      "properties": {
        "base_url": {
          "type": [
            "string",
            "null"
          ]
        },
        "expected_assurance_level": {
          "type": [
            "string",
            "null"
          ]
        }
      },
      "type": "object"
    }
  },
  "required": [
    "profile",
    "generated_at",
    "target",
    "summary",
    "results"
  ],
  "title": "TSPP Conformance Report",
  "type": "object"
}